#tag WindowCode
	#tag Property, Flags = &h0
		CancelLoadURL As string
	#tag EndProperty

	#tag Property, Flags = &h0
		DocBegin As string
	#tag EndProperty

	#tag Property, Flags = &h21
		Private oAuth_Authorize As Dropbox_oAuth
	#tag EndProperty


#tag EndWindowCode

#tag Events AuthorizeButton
	#tag Event
		Sub Action()
		  // AUTHORIZE APP
		  //oAuth_Authorize = New Dropbox_oAuth
		  //oAuth_Authorize.Secure = True
		  //oAuth_Authorize.Port = 443
		  //oAuth_Authorize.ConnectionType = 3
		  //oAuth_Authorize.Get(oAuth_Authorize.oAuth_URL+"?client_id=1l8g9qrlnuxynio&response_type=code&redirect_uri=https://www.google.com")
		  
		  
		  HTMLViewer1.LoadURL("https://www.dropbox.com/1/oauth2/authorize?client_id=1l8g9qrlnuxynio&response_type=token&redirect_uri=https://www.google.com")
		  ReloadTimer.Period = 2000
		  ReloadTimer.mode = Timer.ModeSingle
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events HTMLViewer1
	#tag Event
		Sub DocumentComplete(URL as String)
		  AuthorizeButton.Visible = False
		  AuthorizeButton.Enabled = True
		  
		  ContinueButton.Visible = True
		  ContinueButton.Enabled = True 
		End Sub
	#tag EndEvent
	#tag Event
		Function CancelLoad(URL as String) As Boolean
		  CancelLoadURL = CancelLoadURL + URL  + EndOfLine
		End Function
	#tag EndEvent
	#tag Event
		Sub Open()
		  if me.IsAvailable then
		    dim agent as string
		    
		    agent = "Mozilla/5.0 (Macintosh; U; Intel Mac OS X 10_5_8; en-us) AppleWebKit/533.16 (KHTML, like Gecko) Version/5.0 Safari/533.16"
		    
		    me.UserAgent = agent
		  end if
		End Sub
	#tag EndEvent
	#tag Event
		Sub DocumentBegin(URL as String)
		  DocBegin = DocBegin + Url + EndOfLine
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ContinueButton
	#tag Event
		Sub Action()
		  // AUTHORIZE APP
		  //oAuth_Authorize = New Dropbox_oAuth
		  //oAuth_Authorize.Secure = True
		  //oAuth_Authorize.Port = 443
		  //oAuth_Authorize.ConnectionType = 3
		  //oAuth_Authorize.Get(oAuth_Authorize.oAuth_URL+"?client_id=1l8g9qrlnuxynio&response_type=code&redirect_uri=https://www.google.com")
		  
		  HTMLViewer1.ExecuteJavaScript("location.reload();")
		  
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ReloadTimer
	#tag Event
		Sub Action()
		  Window1.HTMLViewer1.ExecuteJavaScript("location.reload();")
		  
		End Sub
	#tag EndEvent
#tag EndEvents

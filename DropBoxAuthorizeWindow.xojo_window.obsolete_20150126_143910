	#tag Event
		Sub Action()
		  HTMLViewer1.ExecuteJavaScript("location.reload();")
		  
		  // PARSE TOKEN FROM RETURN URL
		  
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents

#tag WindowCode
	#tag Event
		Sub Open()
		  HTMLViewer1.LoadURL("https://www.dropbox.com/1/oauth2/authorize?client_id=1l8g9qrlnuxynio&response_type=token&redirect_uri=https://www.google.com")
		  
		  ChartOpenCircle1.PercentValue = 100.99
		  ChartOpenCircle1.ChartColor =&c0280E4 // &c00FF00AA
		  ChartOpenCircle1.TextColor = &c0280E4//&c00FF0077
		  
		  ChartOpenCircle1.Animate
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub ParseToken(PageContent as String)
		  ChartOpenCircle1.Visible = False
		  ChartOpenCircle1.Enabled = False
		  // PARSE ACCESS TOKEN
		  Dim Parse_AccessToken_RegEx as RegEx
		  Dim Parse_AccessToken_RegExMatch as RegExMatch
		  Dim Parse_AccessToken_HitText as String
		  Parse_AccessToken_RegEx = New RegEx
		  Parse_AccessToken_RegEx.Options.CaseSensitive = False
		  Parse_AccessToken_RegEx.Options.Greedy = True
		  Parse_AccessToken_RegEx.Options.StringBeginIsLineBegin = True
		  Parse_AccessToken_RegEx.Options.StringEndIsLineEnd = True
		  Parse_AccessToken_RegEx.Options.MatchEmpty = True
		  Parse_AccessToken_RegEx.Options.TreatTargetAsOneLine = False
		  Parse_AccessToken_RegEx.SearchPattern = "(?<=#access_token=).+(?=&token_type)"
		  
		  Parse_AccessToken_RegExMatch = Parse_AccessToken_RegEx.Search(PageContent)
		  if Parse_AccessToken_RegExMatch <> nil then
		    Parse_AccessToken_HitText = Trim(Parse_AccessToken_RegExMatch.SubExpressionString(0))
		    Common_Module.DropboxAPISocket.Dropbox_Auth_Token = Parse_AccessToken_HitText
		  end if
		  
		  // PARSE TOKEN TYPE
		  Dim Parse_TokenType_RegEx as RegEx
		  Dim Parse_TokenType_RegExMatch as RegExMatch
		  Dim Parse_TokenType_HitText as String
		  Parse_TokenType_RegEx = New RegEx
		  Parse_TokenType_RegEx.Options.CaseSensitive = False
		  Parse_TokenType_RegEx.Options.Greedy = True
		  Parse_TokenType_RegEx.Options.StringBeginIsLineBegin = True
		  Parse_TokenType_RegEx.Options.StringEndIsLineEnd = True
		  Parse_TokenType_RegEx.Options.MatchEmpty = True
		  Parse_TokenType_RegEx.Options.TreatTargetAsOneLine = False
		  Parse_TokenType_RegEx.SearchPattern = "(?<=&token_type=).+(?=&uid=)"
		  
		  Parse_TokenType_RegExMatch = Parse_TokenType_RegEx.Search(PageContent)
		  if Parse_TokenType_RegExMatch <> nil then
		    Parse_TokenType_HitText = Trim(Parse_TokenType_RegExMatch.SubExpressionString(0))
		    Common_Module.DropboxAPISocket.Dropbox_Auth_TokenType = Parse_TokenType_HitText
		    
		  end if
		  
		  // PARSE UID
		  Dim Parse_TokenUID_RegEx as RegEx
		  Dim Parse_TokenUID_RegExMatch as RegExMatch
		  Dim Parse_TokenUID_HitText as String
		  Parse_TokenUID_RegEx = New RegEx
		  Parse_TokenUID_RegEx.Options.CaseSensitive = False
		  Parse_TokenUID_RegEx.Options.Greedy = True
		  Parse_TokenUID_RegEx.Options.StringBeginIsLineBegin = True
		  Parse_TokenUID_RegEx.Options.StringEndIsLineEnd = True
		  Parse_TokenUID_RegEx.Options.MatchEmpty = True
		  Parse_TokenUID_RegEx.Options.TreatTargetAsOneLine = False
		  Parse_TokenUID_RegEx.SearchPattern = "(?<=&uid=).+"
		  
		  Parse_TokenUID_RegExMatch = Parse_TokenUID_RegEx.Search(PageContent)
		  if Parse_TokenUID_RegExMatch <> nil then
		    Parse_TokenUID_HitText = Trim(Parse_TokenUID_RegExMatch.SubExpressionString(0))
		    Common_Module.DropboxAPISocket.Dropbox_Auth_TokenUID = Parse_TokenUID_HitText
		    
		  end if
		  
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		CancelLoadURL As string
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Results As String
	#tag EndProperty


#tag EndWindowCode

#tag Events HTMLViewer1
	#tag Event
		Sub DocumentComplete(URL as String)
		  CancelLoadURL = URL
		  // PARSE TOKEN INFO FROM REDIRECT URL
		  ParseToken(CancelLoadURL)
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  if me.IsAvailable then
		    dim agent as string
		    agent = "Mozilla/5.0 (Macintosh; U; Intel Mac OS X 10_5_8; en-us) AppleWebKit/533.16 (KHTML, like Gecko) Version/5.0 Safari/533.16"
		    me.UserAgent = agent
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
